{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },

    {
      "src": "/",
      "dest": "/index.html"
    },

    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
