{
  "version": 2,
  "env": {

    "VERCEL_BUILD_ENABLE_FLUTTER": "1"
  },
  
  "rewrites": [
   
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    
    {
      "source": "/(.*\\..*)", 
      "destination": "/$1"
    },
    
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
