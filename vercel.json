{
  "version": 2,

  "outputDirectory": "out",

  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm run build"
      }
    }
  ],

  "rewrites": [
  
    { "source": "/api/(.*)", "destination": "/api/$1" },

    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
