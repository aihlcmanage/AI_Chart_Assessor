{
  "builds": [
    {
      "src": "**/*.dart",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "flutter build web --web-renderer html --release",
        "outputDirectory": "build/web" 
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
